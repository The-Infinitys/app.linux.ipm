{
    "about": {
        "name": "Infinity Package Manager's Default Package",
        "id": "ipm-default-pack",
        "version":"0.1.0",
        "author": {
            "name": "The Infinity's",
            "id": "the-infinitys"
        },
        "description": "README.md",
        "license": "LICENSE",
        "dependencies": [
            {
                "depend_type":"must",
                "name":"echo",
                "version":"*"
            },
            {
                "depend_type":"should",
                "name":"apt",
                "version":"*"
            },
            {
                "depend_type":"conflict",
                "name":"ipm-anti",
                "version":"*"
            },
            {
                "depend_type":"replace",
                "name":"ipm-default-old",
                "version":"*"
            }
        ],
        "architecture":["*"]
    },
    "files": {
        "global": [
            {
                "file_type":"bin",
                "from": "source/exec.sh",
                "to": [
                    "bin/ipm-example",
                    "usr/bin/ipm-example"
                ]
            }
        ],
        "local": [
        ]
    }
}